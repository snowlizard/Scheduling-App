<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="537.0" prefWidth="879.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="c195.schedulingapp.Controllers.Customers">
    <children>
        <TableView fx:id="customers" layoutX="10.0" layoutY="55.0" prefHeight="470.0" prefWidth="861.0">
            <columns>
                <TableColumn prefWidth="50.0" fx:id="id" text="ID" />
                <TableColumn prefWidth="100.0" fx:id="name" text="Name" />
                <TableColumn prefWidth="200.0" fx:id="address" text="Address" />
                <TableColumn prefWidth="100.0" fx:id="postal_code" text="Postal Code" />
                <TableColumn prefWidth="160.0" fx:id="phone" text="Phone" />
                <TableColumn prefWidth="130.0" fx:id="division_id" text="Division ID" />
                <TableColumn prefWidth="130.0" fx:id="country_id" text="Country ID" /> 
            </columns>
        </TableView>
        <Button onAction="#previousScene"  fx:id="back" layoutX="10.0" layoutY="25.0" mnemonicParsing="false" text="Back" />
        <Button onAction="#addCustomer"  fx:id="add" layoutX="827.0" layoutY="25.0" mnemonicParsing="false" text="Add" />
        <Button onAction="#editCustomer" fx:id="modify" layoutX="758.0" layoutY="25.0" mnemonicParsing="false" text="Modify" />
        <Button onAction="#deleteCustomer" fx:id="delete" layoutX="690.0" layoutY="25.0" mnemonicParsing="false" text="Delete" />
    </children>
</AnchorPane>
